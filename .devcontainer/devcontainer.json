{
    "name": "Micro Videos Admin - PHP8",
    "dockerComposeFile": ["../docker-compose.yml", "docker-compose.yml"],
    "service": "app",
    "workspaceFolder": "/var/www",
    "features": {},
    "customizations": {
        "vscode": {
            "extensions": [
                "felipe.nascimentov.vscode-php",
                "bmewburn.vscode-intelephense-client",
                "xdebug.php-debug",
                "psalm.psalm",
                "php.mssql",
                "phpdebugbar.phpdebugbar",
                "zhuangtongfa.material-theme",
                "brianlhoward.vscode-php-formatter",
                "ikapp.vscode-phpunit",
                "neilbrayfield.php-docblocker",
                "php.solargraph",
                "tabnine.tabnine-vscode",
                "xyz.local-history",
                "esbenp.prettier-vscode",
                "ms-azuretools.vscode-docker",
                "eamodio.gitlens",
                "mhutchie.git-graph",
                "usernamehw.errorlens",
                "jebbs.plantuml",
                "qhoekman.language-plantuml",
                "rvest.vs-code-prettier-eslint",
                "humao.rest-client",
                "donjayamanne.githistory",
                "dbaeumer.vscode-eslint",
                "editorconfig.editorconfig",
                "michelemelluso.gitignore",
                "antfu.iconify",
                "tal7aouy.icons",
                "devsense.composer-php-vscode",
                "recca0120.vscode-phpunit",
                "sdras.night-owl"
            ],
            "settings": {
                "editor.formatOnSave": true,
                "php.validate.executablePath": "/usr/local/bin/php",
                "php.solargraph.languageServerPath": "/usr/local/bin/solargraph",
                "[php]": {
                    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
                },
                "intelephense.environment.phpVersion": "8.3.10",
                "files.exclude": {},
                "intelephense.files.associations": [
                    "*.php",
                    "*.phtml"
                ],
                "workbench.colorTheme": "Night Owl"
            }
        }
    },
    "postCreateCommand": "/bin/sh /var/www/.docker/start.sh",
    "remoteUser": "www-data",
    "shutdownAction": "none"
}
